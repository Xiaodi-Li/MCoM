{
    "name": "mnist_logistic",
    "n_gpu": 1,
    "supervised_arch": {
        "type": "LogisticRegression",
        "args": {
            "input_dim": 784,
            "num_classes": 10
        }
    },
    "supervised_data_loader": {
        "type": "KerasMNISTDataLoader",
        "args": {
            "method": "supervised",
            "batch_size": 128,
            "labeled_ratio": 0.10,
            "shuffle": true,
            "validation_split": 0.10,
            "num_workers": 8
            }
    },
    "supervised_optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0010939661837841578,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "supervised_metrics": ["accuracy"],
    "supervised_lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 10,
            "gamma": 0.1
        }
    },
    "supervised_trainer": {
        "type": "Trainer",
        "module_name": "trainer",
        "epochs": 30,
        "save_dir": "saved/",
        "save_period": 10,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 10,
        "save_single_checkpoint": true,
        "tensorboard": false,
        "log_step": 50
    },
    "supervised_model_load_best": true
}
